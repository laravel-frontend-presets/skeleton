{
    "name": "jlndk/laravel-polymer-webpack-frontend-preset",
    "description": "Laravel 5.5.X Front-end preset for Polymer 2.x using webpack.",
    "keywords": ["laravel", "preset", "polymer", "webpack"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Lindenskov Nielsen",
            "email": "jonas@jonasln.dk"
        }
    ],
    "require": {
        "laravel/framework": ">=5.5"
    },
    "autoload": {
        "psr-4": {
            "Jlndk\\PolymerWebpackPreset\\": "src/"
        }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Jlndk\\PolymerWebpackPreset\\PolymerPresetServiceProvider"
       ]
      }
    }

}
